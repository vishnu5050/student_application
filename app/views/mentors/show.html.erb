<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @mentor.name %>
</p>

<p>
  <strong>Age:</strong>
  <%= @mentor.age %>
</p>

<p>
  <strong>Gender:</strong>
  <%= @mentor.gender %>
</p>

<p>
  <strong>Dob:</strong>
  <%= @mentor.dob %>
</p>

<p>
  <strong>Bio:</strong>
  <%= @mentor.bio %>
</p>

<p>
  <strong>Country:</strong>
  <%= @mentor.country %>
</p>

<p>
  <strong>Students under this mentor :</strong><ul>
  <% @mentor.students.find_each do |f| %>
    <li><%= f.name %></li>
  <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_mentor_path(@mentor), class: 'btn-btn-primary' %> |
<%= link_to 'Back', mentors_path, class: 'btn-btn-primary' %>
